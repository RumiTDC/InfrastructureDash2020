                <component
                  refId="Package\Build Dimension Members\Load Dimension Prep Table\EDW_Stage_History prep Customer"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="EDW_Stage_History prep Customer"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset">[dbo].[Customer]</property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable">User::StagePrepViewName</property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">/*Temp Solution to Customers / Accounts From CRM*/

SELECT 
co.DynamicsNavCompanyID AS CompanyId
, f.name AS CustomerName
, ISNULL(CAST(f.accountnumber AS nvarchar(50)), f.accountid) CustomerNumber
, f.nrq_responsibilitycentername CustomerResponsibilityCenter
, f.nrq_industrygroupname CustomerIndustryCode
, f.industrycodename   CustomerIndustry
, f.nrq_departmentcodename CustomerDepartment
, CustomerSalesResponsibleCode = N'Unknown' 
, CustomerSalesResponsibleName = N'Unknown'
, f.paymenttermscodename CustomerPaymentTermsCode
, f.address1_addressid CustomerAddress
, f.address1_city CustomerCity
, f.address1_postalcode CustomerPostalCode
, f.address1_country CustomerCountry
, f.nrq_discountgroupname CustomerDiscountGroup
FROM crm.FilteredAccount f
INNER JOIN utility.DynamicsNavCompany co on co.CompanyName=f.nrq_navcompanyname
WHERE f.HistoryCurrentRec = 1
AND co.HistoryCurrentRec=1
AND CONCAT(co.DynamicsNavCompanyID, '-', ISNULL(f.accountnumber, f.accountid)) NOT IN (
SELECT	CONCAT(c.[CompanyId] ,'-' , c.[No_])
FROM nav.[Customer] c
LEFT OUTER JOIN nav.[Default Dimension] dd on C.No_=dd.No_ AND c.[CompanyId] = dd.[CompanyId] AND [Table ID] = 18 AND [Dimension Code]='INDUSTRY GROUP' AND dd.HistoryCurrentRec = 1
LEFT OUTER JOIN nav.[Dimension Value] dv on dd.[Dimension Value Code]=dv.[Code] AND dv.CompanyId=dd.CompanyId AND dv.[Dimension Code]='INDUSTRY GROUP' AND dv.HistoryCurrentRec=1
LEFT OUTER JOIN nav.Salesperson_Purchaser sp on c.[Salesperson Code] = sp.Code AND c.CompanyId=sp.CompanyId AND sp.HistoryCurrentRec=1
LEFT OUTER JOIN nav.[Customer Discount Group] dc on c.[Customer Disc_ Group] = dc.Code AND c.CompanyId=dc.CompanyId AND dc.HistoryCurrentRec=1
WHERE c.HistoryCurrentRec = 1
)
--AND 1&gt;2 -- Filter Error to 1 april 2016
ORDER BY 3</property>
